<% if @user.errors.any? %>
<b>Errors:</b>
<ul>
	<% @user.errors.full_messages.each do |msg| %>
	<li><%= msg %></li>
	<% end %>
</ul>
<% end %>

<%= form_for @user do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :uscf_id, 'USCF ID' %>
	<%= f.number_field :uscf_id %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.label :password_confirmation, 'Confirm Password' %>
	<%= f.password_field :password_confirmation %>

	<%= f.label :admin, 'Admin', class: 'secret' %>
	<%= f.check_box :admin, class: 'secret' %>

	<%= f.submit 'Submit', id: 'submit' %>

<% end %>

Or go to the <%= link_to "Sign In", signin_path %> page.